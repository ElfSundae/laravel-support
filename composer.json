{
    "name": "elfsundae/laravel-support",
    "type": "library",
    "description": "Laravel Support",
    "keywords": ["laravel", "support"],
    "homepage": "https://github.com/ElfSundae/laravel-support",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "doctrine/dbal": "~2.6",
        "elfsundae/laravel-agent": "~2.2",
        "laravel/framework": "5.5.*",
        "predis/predis": "~1.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "caouecs/laravel-lang": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\Laravel\\Support\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ElfSundae\\Laravel\\Support\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "ElfSundae\\Laravel\\Support\\SupportServiceProvider"
            ],
            "aliases": {
            },
            "dont-discover": [
            ]
        },
        "suggest-dev": {
            "barryvdh/laravel-debugbar": "~3.0",
            "barryvdh/laravel-ide-helper": "~2.4"
        }
    },
    "suggest": {
        "elfsundae/httpclient": "~1.1",
        "elfsundae/intervention-image": "~1.1",
        "elfsundae/laravel-agent": "~2.2",
        "elfsundae/laravel-api": "~1.6",
        "elfsundae/laravel-asset-version": "~1.0",
        "elfsundae/laravel-captcha": "~2.2",
        "elfsundae/laravel-datatables": "~2.1",
        "elfsundae/laravel-gravatar": "~2.0",
        "elfsundae/laravel-hashid": "~1.0",
        "elfsundae/laravel-helper": "~1.3",
        "guzzlehttp/guzzle": "~6.3",
        "iatstuti/laravel-nullable-fields": "~1.5",
        "intervention/image": "~2.4",
        "rap2hpoutre/laravel-log-viewer": "~0.11",
        "simplesoftwareio/simple-qrcode": "~1.5",
        "spatie/laravel-backup": "~5.0"
    }
}
