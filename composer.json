{
    "name": "elfsundae/laravel-support",
    "type": "library",
    "description": "Laravel Support",
    "keywords": ["laravel", "support"],
    "homepage": "https://github.com/ElfSundae/laravel-support",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "5.5.*",
        "doctrine/dbal": "~2.6",
        "elfsundae/laravel-apps": "dev-master",
        "elfsundae/laravel-agent": "~2.2"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "caouecs/laravel-lang": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\Laravel\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElfSundae\\Laravel\\Support\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "lang": "mkdir -p resources/lang && cp -af vendor/caouecs/laravel-lang/src/zh-CN resources/lang"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "ElfSundae\\Laravel\\Support\\SupportServiceProvider"
            ],
            "aliases": {
            },
            "dont-discover": [
            ]
        }
    },
    "suggest": {
        "barryvdh/laravel-debugbar": "~3.0 --dev",
        "barryvdh/laravel-ide-helper": "~2.4 --dev",
        "guzzlehttp/guzzle": "Guzzle, an extensible PHP HTTP client (~6.3)",
        "elfsundae/httpclient": "A wrapper of Guzzle HTTP client (~1.1)",
        "intervention/image": "PHP image handling and manipulation (~2.4)",
        "elfsundae/intervention-image-filters": "Intervention Image Filters (~2.0)",
        "elfsundae/laravel-agent": "~2.2",
        "elfsundae/laravel-api": "~1.6",
        "elfsundae/laravel-asset-version": "Laravel static assets versioning via query string: 'app.js' â†’ 'app.js?d41d8cd98f'",
        "elfsundae/laravel-captcha": "A patched package to mews/captcha (~2.2)",
        "elfsundae/laravel-datatables": "extended yajra/laravel-datatables (~2.3)",
        "elfsundae/laravel-gravatar": "The easiest way to generate Gravatar avatar URL (~2.0)",
        "elfsundae/laravel-hashid": "Obfuscate your data by generating reversible, non-sequential, URL-safe identifiers.",
        "elfsundae/laravel-helper": "~1.3",
        "iatstuti/laravel-nullable-fields": "Handles saving empty fields as null for Eloquent models (~1.5)",
        "rap2hpoutre/laravel-log-viewer": "Laravel 5 log viewer (~0.11)",
        "simplesoftwareio/simple-qrcode": "An easy-to-use PHP QrCode generator (~2.0)",
        "spatie/laravel-backup": "A package to backup your Laravel app (~5.1)"
    }
}
